{
  "name": "kyper-matter",
  "version": "0.1.7",
  "description": "Library to provide simple application functionality like authentication and local/session/token storage for Tesselate applications.",
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/matter.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/matter.min.js --config webpack.config.production.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "watch:umd": "npm run build:umd -- --stats --progress --colors --watch",
    "watch:lib": "npm run build:lib -- --stats --progress --colors --watch",
    "watch": "npm run watch:umd",
    "clean": "rimraf lib dist coverage",
    "prepublish": "npm run clean && npm run build",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kypertech/matter.git"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "matter",
    "grout",
    "tessellate",
    "kyper",
    "authentication",
    "user management",
    "application management"
  ],
  "author": "Prescott Prue <sprue.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kypertech/matter/issues"
  },
  "homepage": "https://github.com/kypertech/matter",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.0.5",
    "babelify": "^6.0.0",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.9.3",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "del": "^1.1.1",
    "glob": "^5.0.14",
    "gulp": "^3.8.10",
    "gulp-awspublish": "^3.0.0",
    "gulp-bump": "^1.0.0",
    "gulp-esdoc": "^0.2.0",
    "gulp-eslint": "^1.0.0",
    "gulp-file": "^0.2.0",
    "gulp-filter": "^3.0.0",
    "gulp-jscs": "^2.0.0",
    "gulp-livereload": "^3.4.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-mocha": "^2.0.0",
    "gulp-notify": "^2.1.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-shell": "^0.4.3",
    "isparta": "^3.0.3",
    "jsdom": "^7.0.2",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-plugins": "^0.6.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^0.5.2",
    "karma-es6-shim": "^0.1.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "mocha": "^2.1.0",
    "phantomjs": "^1.9.18",
    "rimraf": "^2.4.3",
    "run-sequence": "^1.0.2",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^1.11.0"
  },
  "dependencies": {
    "hellojs": "^1.8.2",
    "jwt-decode": "^1.4.0",
    "lodash": "^3.10.1",
    "superagent": "^1.3.0"
  }
}
